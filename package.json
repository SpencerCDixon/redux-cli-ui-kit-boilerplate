{
  "name": "personal-ui-kit",
  "version": "0.0.1",
  "description": "personal UI Kit",
  "main": "./dist/",
  "scripts": {
    "babel": "better-npm-run babel",
    "prebuild": "npm run lint && npm test && npm run clean",
    "build": "better-npm-run build",
    "postbuild": "npm run clean:dev",
    "clean": "rimraf dist",
    "fonts": "cpx './src/fonts/*/*.{eot,ttf,woff}' ./dist/fonts",
    "images": "cpx './src/images/*.{png,jpg}' ./dist/images",
    "docs": "react-docgen `find ./src/components -type f -depth 2 | grep js | grep -v 'index\\|example\\|test\\|Documentation'` | bin/buildDocs.sh",
    "lint": "eslint src/components",
    "start": "better-npm-run start",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch"
  },
  "betterScripts": {
    "start": {
      "command": "webpack-dev-server --config webpack.config.dev.js --progress",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "babel": {
      "command": "babel ./src --out-dir ./dist --ignore examples/*,*Documentation.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start karma.config.js",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "build": {
      "command": "npm run babel && npm run fonts && npm run images"
    }
  },
  "keywords": [
    "react-components",
    "ui",
    "ui-kit",
    "design"
  ],
  "author": "Spencer Dixon",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "better-npm-run": "0.0.8",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "cheerio": "^0.20.0",
    "cpx": "^1.3.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-import-resolver-webpack": "^0.2.4",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.5",
    "handlebars": "^4.0.5",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "prettyjson": "^1.1.3",
    "react-addons-test-utils": "^0.14.8",
    "react-docgen": "^2.7.0",
    "react-router": "^3.0.0-alpha.1",
    "react-router-bootstrap": "^0.23.0",
    "react-syntax-highlighter": "^2.1.0",
    "react-transform-hmr": "^1.0.4",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "peerDependencies": {
    "react": "^0.14.x || ^15.0.0",
    "react-dom": "^0.14.x || ^15.0.0",
    "classnames": "^2.2.5"
  }
}
